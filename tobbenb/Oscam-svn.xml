<?xml version="1.0" encoding="utf-8"?>
<Container>
  <Name>Oscam-svn</Name>
  <Description>Oscam is a card server and client for decoding DVB streams over DVB-S, DVB-C, DVB-T or IPTV.[br]&#13;
		http://www.streamboard.tv/oscam/[br]&#13;
		[b][span style=\'color: #E80000;\']Directions:[/span][/b][br]&#13;
		[b]/config[/b] : This is where Oscam will store it\'s configuration.[br][br]&#13;
</Description>
  <Registry>https://registry.hub.docker.com/u/tobbenb/oscam-svn/</Registry>
  <Repository>tobbenb/oscam-svn</Repository>
  <BindTime>true</BindTime>
  <Privileged>false</Privileged>
  <Environment/>
  <Networking>
    <Mode>bridge</Mode>
    <Publish>
      <Port>
        <HostPort>8888</HostPort>
        <ContainerPort>8888</ContainerPort>
        <Protocol>tcp</Protocol>
      </Port>
      <Port>
        <HostPort>10000</HostPort>
        <ContainerPort>10000</ContainerPort>
        <Protocol>tcp</Protocol>
      </Port>
    </Publish>
  </Networking>
  <Data>
    <Volume>
      <HostDir>/mnt/cache/appdata/oscam/</HostDir>
      <ContainerDir>/config</ContainerDir>
      <Mode>rw</Mode>
    </Volume>
  </Data>
  <Version></Version>
  <WebUI>http://[IP]:[PORT:8888]</WebUI>
  <Banner></Banner>
  <Icon>http://i.imgur.com/8LadrLg.png?1</Icon>
</Container>
